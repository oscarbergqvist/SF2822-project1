  57      yieldZC(i)      ..  0.2*x1(i) + 0.1*x2(i) =e= zc(i);
  58      usplit(i)       ..  u1(i) + u2(i) =e= zb(i);
  59      capacityV(i)    ..  zc(i) + u2(i) =l= 300;
  60      yieldVA(i)      ..  0.9*u2(i) + 0.4*zc(i) =e= va(i);
  61      yieldVB(i)      ..  0.1*u2(i) + 0.6*zc(i) =e= vb(i);
  62      demandPA(i)     ..  pa(i) =l= 250;
  63      demandPB(i)     ..  pb(i) =l= d(i);
  64      maxsaleB(i)     ..  u1(i) + vb(i) =g= pb(i);
